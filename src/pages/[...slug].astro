export async function getStaticPaths() {
  const pages = [
    {
      slug: undefined,
    }
  ];
  return pages.map(({ slug }) => {
    return {
      params: { slug }
    };
  });
}
const { path } = Astro.params;
const page = pages.find((page) => page.slug === slug);
if (!page) return Astro.redirect("/404");
